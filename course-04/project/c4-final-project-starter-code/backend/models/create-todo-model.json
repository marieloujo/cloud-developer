{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "group",
  "type": "object",
  "properties": {
    "userId": {
        "type": "string"
    },
    "todoId": {
        "type": "string"
    },
    "createdAt": {
        "type": "string"
    },
    "name": {
        "type": "string",
        "minLength": 1
    },
    "dueDate": {
        "type": "string",
        "minLength": 1
    },
    "done": {
        "type": "boolean"
    },
    "attachmentUrl": {
        "type": "string"
    }
  },
  "required": [
    "name",
    "dueDate"
  ],
  "additionalProperties": false
}
